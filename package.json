{
  "name": "poker-host",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "kill": "pkill -f 'expo start' || true",
    "web:clean": "npm run kill && expo start --web --clear",
    "web:lan": "npm run kill && expo start --web --host lan --clear",
    "ios:clean": "npm run kill && expo start --ios --clear",
    "ios:lan": "npm run kill && expo start --ios --host lan --clear",
    "ios:tunnel": "npm run kill && expo start --tunnel --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "server": "node websocket-server.js",
    "dev": "concurrently \"npm run server\" \"npm start\""
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "concurrently": "^8.2.2",
    "expo": "^54.0.7",
    "expo-apple-authentication": "~8.0.7",
    "expo-auth-session": "~7.0.8",
    "expo-clipboard": "~8.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "~15.0.7",
    "express": "^5.1.0",
    "firebase": "^12.3.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets-core": "^1.6.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/ngrok": "^4.1.3",
    "@types/react": "~19.1.10",
    "@types/react-native": "~0.72.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
